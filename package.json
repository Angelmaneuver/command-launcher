{
	"name": "command-launcher",
	"displayName": "Command Launcher",
	"description": "A launcher extension that manages commands for the command palette.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"icon": "resource/AppIcon.png",
	"categories": [
		"Other"
	],
	"keywords": [
		"Command Launcher",
		"Mac"
	],
	"activationEvents": [
		"onCommand:command-launcher.launcher",
		"onCommand:command-launcher.edit"
	],
	"preview": true,
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "command-launcher.launcher",
				"title": "Command Launcher"
			},
			{
				"command": "command-launcher.edit",
				"title": "Command Launcher - Edit"
			}
		],
		"configuration": {
			"title": "Command Launcher",
			"properties": {
				"command-launcher.commands": {
					"type": "object",
					"default": {},
					"description": "Registered command set."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/lodash": "^4.14.182",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.6.4",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	}
}
